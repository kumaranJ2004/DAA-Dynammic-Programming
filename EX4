#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[1000];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int dp[1000];
    int maxLen = 1;

   
    for (int i = 0; i < n; i++) {
        dp[i] = 1;
    }


    for (int i = 1; i < n; i++) {
        for (int j = 0; j < i; j++) {
            if (arr[j] <= arr[i] && dp[i] < dp[j] + 1) {
                dp[i] = dp[j] + 1;
            }
        }
        if (dp[i] > maxLen)
            maxLen = dp[i];
    }

    printf("%d\n", maxLen);
    return 0;
}
